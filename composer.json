{
    "name": "orchestra/imagine",
    "description": "Imagine (Wrapper) Component for Laravel",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "image-manipulation", "image"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "imagine/imagine": "^1.2",
        "orchestra/support-core": "^7.0",
        "orchestra/support-providers": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Orchestra\\Imagine\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
