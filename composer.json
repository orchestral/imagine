{
    "name": "orchestra/imagine",
    "description": "Imagine (Wrapper) Component for Laravel 5",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "image-manipulation", "image"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "require": {
        "php": ">=7.0",
        "imagine/imagine": "~0.5",
        "orchestra/support-core": "~3.6.0",
        "orchestra/support-providers": "~3.6.0"
    },
    "require-dev": {
        "illuminate/container": "~5.6.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Orchestra\\Imagine\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6-dev"
        }
        "laravel": {
            "providers": [
                "Orchestra\\Imagine\\ImagineServiceProvider"
            ],
            "aliases": {
                "Imagine":"Orchestra\\Imagine\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
